def count_letter(word, charA, charC, charG, charT) :
	countA = 0
	countC = 0
	countG = 0
	countT = 0
	for A in word:
		countA += (charA == A)
	for C in word:
		countC += (charC == C)
	for G in word:
		countG += (charG == G)
	for T in word:
		countT += (charT == T)
	return countA, countC, countG, countT

print count_letter('', 'A', 'C', 'G', 'T')
